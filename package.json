{
  "name": "my-mevn-crud-app",
  "author": "heyitskippy",
  "version": "0.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --names server,client --prefix-colors yellow,green 'yarn dev:server' 'yarn dev:client'",
    "dev:server": "vite --config vite.config.server.ts",
    "dev:client": "vite --config vite.config.client.ts --host",
    "build-only:client": "vite build --config vite.config.client.ts",
    "build-only:server": "vite build --config vite.config.server.ts",
    "build": "run-p type-check 'build-only:server {@}' 'build-only:client {@}' --",
    "preview": "concurrently --names server,client --prefix-colors yellow,green \"node dist/server/index.cjs\" \"vite preview\"",
    "test": "vitest --silent --run",
    "test:server": "vitest --project=server:unit --project=server:integration --silent --run",
    "test:client": "vitest --project=client:unit --run",
    "coverage": "vitest run --coverage",
    "prepare:cypress": "cypress install",
    "test:e2e": "dotenv -e .env.production -- start-test preview http://localhost:4173/cypress 'cypress run --e2e'",
    "test:e2e:dev": "dotenv -e .env.development -- start-test 'yarn dev' http://localhost:3870 'cypress open --e2e'",
    "type-check": "vue-tsc --build",
    "lint": "eslint ./client/src/ --fix",
    "format": "prettier --write client/src/",
    "prepare:mongo:dev": "dotenv -e .env.development -- docker compose -f server/docker-compose.yml up -d",
    "prepare:mongo:prod": "dotenv -e .env.production -- docker compose -f server/docker-compose.yml up -d"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^12.8.2",
    "@vueuse/integrations": "^12.8.2",
    "@vueuse/router": "^13.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "lodash-es": "^4.17.21",
    "mongoose": "^8.10.1",
    "nprogress": "^0",
    "pinia": "^2.3.1",
    "tailwindcss": "^4.0.8",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@pinia/testing": "^1.0.1",
    "@tailwindcss/vite": "^4.0.8",
    "@tsconfig/node22": "^22.0.0",
    "@types/body-parser": "^1",
    "@types/cors": "^2",
    "@types/express": "^4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4",
    "@types/morgan": "^1",
    "@types/node": "^22.13.1",
    "@types/nprogress": "^0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/utils": "^8.29.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "3.0.5",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.0.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.16.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.10",
    "supertest": "^7.0.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-node": "^4.0.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "packageManager": "yarn@4.6.0"
}
